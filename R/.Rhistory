df$DS_log <- log10(df$DS + 0.0001)
df$Area_log <- log10(df$Area + 0.0001)
df$ITM_log <- sign(df$ITM)*log10(abs(df$ITM) + 1)
df$LR_log <- log10(df$LR + 0.0001)
df$SR_log <- log10(df$SR + 0.0001)
df$bwcent_log <- log10(df$bw_cent + 0.0001)
